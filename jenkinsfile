pipeline {
	agent any
	
	stages {
	    stage('Checkout') {
	        steps {
			checkout scm			       
		      }}
		stage('Build') {
	           steps {
			  sh '/home/shantanu/Documents/jenkins/mvn install'
	                 }}
		stage('Deployment'){
		    steps {
			
			sh 'cp target/samsung.war /home/shantanu/Documents/jenkins/apache-tomcat-9.0.70/webapps'
	}
}}}
